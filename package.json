{
  "name": "ai-voice-feedback-dashboard",
  "version": "1.0.0",
  "description": "AI-powered voice and file analysis dashboard with real-time feedback",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server.js",
    "start": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .vue,.js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "analyze": "vite-bundle-analyzer",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.0.18",
    "axios": "^1.6.5",
    "chart.js": "^4.4.1",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.0",
    "@vue/cli-service": "^5.0.9",
    "@vue/test-utils": "^2.4.3",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.33.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "playwright": "^1.41.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.19",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.2.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{vue,js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "vue3",
    "ai",
    "voice-analysis",
    "dashboard",
    "tailwindcss",
    "pinia",
    "chartjs"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT"
}
